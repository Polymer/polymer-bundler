    "espree": "^3.5.2",
    "magic-string": "^0.22.4",
    "mkdirp": "^0.5.1",
    "parse5": "^2.2.2",
    "polymer-analyzer": "^3.0.0-pre.18",
    "rollup": "^0.56.1",
    "source-map": "^0.5.6",
    "vscode-uri": "^1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.30",
